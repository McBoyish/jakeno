{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "no-console": "warn",
    "no-undef": "warn",
    "no-unused-expressions": "warn",
    "no-unused-vars": "off",
    "no-empty-function": "warn"
  },
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "amd": true,
    "es6": true
  }
}
